<html>
  <head>
    <link rel="stylesheet" href="/styles.css">

    <title>Studio</title>

  </head>
  <body>
    <%- include('header') %>
    <div class="update-delete">
      <a href="/studios/<%= studioName %>/update" >
        <span>update</span>
      </a>
      <a href="/studios/<%= studioName %>/delete" >
        <span>delete</span>
      </a>
   
    </div>
    <ul class="movies">
        <% studioMovies.forEach((movie) => { %>
          <li><%= movie.title %>
          <a href="/movies/<%=movie.movie_id%>" >
            <span>open</span>
          </a>
        </li>
        <% }); %>
      </ul>
  

  </body>

</html>